{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "MainApp",
            "request": "launch",
            "mainClass": "app.MainApp",
            "projectName": "studentmanagementsystem1"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "StudentManagementApplication",
            "request": "launch",
            "mainClass": "com.student.StudentManagementApplication",
            "projectName": "student-management"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Open index.html (local file)",
            "file": "${workspaceFolder}/student-management/src/main/resources/static/index.html"
        }
    ]
}